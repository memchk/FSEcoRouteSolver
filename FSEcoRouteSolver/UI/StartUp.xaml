<Window x:Class="FSEcoRouteSolver.UI.StartUp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FSEcoRouteSolver.UI"
        mc:Ignorable="d"
        Title="Enter API Key" Height="75" Width="450" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel Margin="20,0,0,0" Orientation="Horizontal">
            <Label VerticalAlignment="Center">API Key</Label>
            <TextBox x:Name="tAPIKey" Width="250" Height="20" VerticalAlignment="Center"></TextBox>
            <Button x:Name="bStart" Height="30" MinWidth="75" Margin="10,0,0,0" Click="BStart_Click">Start</Button>
        </StackPanel>
    </Grid>
</Window>
